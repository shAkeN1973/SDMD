FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.0001;

boundaryField
{
    airfoil
    {
        type            fixedValue;
        value           uniform 0;
    }
    down
    {
        type            symmetry;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            zeroGradient;
    }
    up
    {
        type            symmetry;
    }
    cyc_half0
    {
        type            cyclic;
    }
    cyc_half1
    {
        type            cyclic;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.0001;
    }
    procBoundary1to0throughcyc_half1
    {
        type            processorCyclic;
        value           uniform 0.0001;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 0.0001;
    }
    procBoundary1to3throughcyc_half1
    {
        type            processorCyclic;
        value           uniform 0.0001;
    }
}


