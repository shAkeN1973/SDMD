FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (3 0 0);

boundaryField
{
    airfoil
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    down
    {
        type            symmetry;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            zeroGradient;
    }
    up
    {
        type            symmetry;
    }
    cyc_half0
    {
        type            cyclic;
    }
    cyc_half1
    {
        type            cyclic;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform (3 0 0);
    }
    procBoundary3to1throughcyc_half0
    {
        type            processorCyclic;
        value           uniform (3 0 0);
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform (3 0 0);
    }
    procBoundary3to2throughcyc_half1
    {
        type            processorCyclic;
        value           uniform (3 0 0);
    }
}


